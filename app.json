{
  "expo": {
    "name": "MovieFlix",
    "slug": "movieflix",
    "version": "1.0.0",
    "supportsPictureInPicture": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/150ba453-1c88-455a-89a0-14147bd74e25",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "movieflix",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.movieflix.app",
      "backgroundColor": "#1C1C1E",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "package": "com.movieflix.app",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/logo.png",
        "backgroundImage": "./assets/images/logo.png",
        "monochromeImage": "./assets/images/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "backgroundColor": "#1C1C1E",
      "intentFilters": [],
      "permissions": [
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "splash": {
      "image": "./assets/images/movies-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#05060f"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/movies-splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#05060f",
          "dark": {
            "image": "./assets/images/movies-splash.png",
            "backgroundColor": "#05060f"
          }
        }
      ],
      "expo-image-picker",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos to your device.",
          "isAccessMediaLocationEnabled": true,
          "isAudioEnabled": false
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 36,
            "targetSdkVersion": 36,
            "minSdkVersion": 24,
            "buildToolsVersion": "36.0.0"
          }
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "150ba453-1c88-455a-89a0-14147bd74e25"
      },
      "agora": {
        "appId": "",
        "tokenEndpoint": ""
      }
    },
    "owner": "mzazimhengas-organization"
  }
}
