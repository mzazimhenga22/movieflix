{
  "expo": {
    "name": "MovieFlix",
    "slug": "movieflix",
    "version": "1.0.0",
    "supportsPictureInPicture": true,
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/56b697d6-b709-4b80-909a-27b3a97856c1",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "orientation": "portrait",
    "icon": "./assets/images/logo-safe.png",
    "scheme": "movieflix",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.movieflix.app",
      "backgroundColor": "#1C1C1E",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow MovieFlix to use your camera to scan a TV sign-in QR code.",
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "package": "com.movieflix.app",
      "usesCleartextTraffic": true,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#E53935",
        "foregroundImage": "./assets/images/logo-safe.png",
        "monochromeImage": "./assets/images/logo-safe.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "backgroundColor": "#1C1C1E",
      "intentFilters": [],
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "splash": {
      "image": "./assets/images/logo-safe.png",
      "resizeMode": "contain",
      "backgroundColor": "#E53935"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "expo-camera",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-safe.png",
          "resizeMode": "contain",
          "backgroundColor": "#E53935",
          "dark": {
            "image": "./assets/images/logo-safe.png",
            "backgroundColor": "#E53935"
          }
        }
      ],
      "expo-image-picker",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos to your device.",
          "isAccessMediaLocationEnabled": true,
          "isAudioEnabled": false
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 36,
            "targetSdkVersion": 36,
            "minSdkVersion": 24,
            "buildToolsVersion": "36.0.0"
          }
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "agora": {
        "appId": "",
        "tokenEndpoint": ""
      },
      "eas": {
        "projectId": "56b697d6-b709-4b80-909a-27b3a97856c1"
      }
    },
    "owner": "vivescharris"
  }
}
